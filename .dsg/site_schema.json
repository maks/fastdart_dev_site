{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Config file for the DSG static site generator. For details, see https://pub.dev/packages/dsg",
    "type": "object",
    "properties": {
        "date_format": {
            "type": "string"
        },
        "yaml_delimeter": {
            "type": "string"
        },
        "site_options": {
            "type": "object"
        }
    },
    "required": ["date_format", "yaml_delimeter"]
}